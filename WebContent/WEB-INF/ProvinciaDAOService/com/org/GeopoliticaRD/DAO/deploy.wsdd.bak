<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ProvinciaDAOService WSDL service -->

  <service name="ProvinciaDAO" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DAO.GeopoliticaRD.org.com"/>
      <parameter name="wsdlServiceElement" value="ProvinciaDAOService"/>
      <parameter name="schemaQualified" value="http://DAO.GeopoliticaRD.org.com,http://models.GeopoliticaRD.org.com"/>
      <parameter name="wsdlServicePort" value="ProvinciaDAO"/>
      <parameter name="className" value="com.org.GeopoliticaRD.DAO.ProvinciaDAOSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ProvinciaDAO"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="findProvincia" qname="operNS:findProvincia" xmlns:operNS="http://DAO.GeopoliticaRD.org.com" returnQName="retNS:findProvinciaReturn" xmlns:retNS="http://DAO.GeopoliticaRD.org.com" returnType="rtns:Provincia" xmlns:rtns="http://models.GeopoliticaRD.org.com" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://DAO.GeopoliticaRD.org.com" type="tns:long" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="findProvincia"/>

      <typeMapping
        xmlns:ns="http://models.GeopoliticaRD.org.com"
        qname="ns:Provincia"
        type="java:com.org.GeopoliticaRD.models.Provincia"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
