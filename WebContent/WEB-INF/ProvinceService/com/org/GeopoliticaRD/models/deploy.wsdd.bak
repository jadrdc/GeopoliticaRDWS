<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ProvinceService WSDL service -->

  <service name="Province" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://models.GeopoliticaRD.org.com"/>
      <parameter name="wsdlServiceElement" value="ProvinceService"/>
      <parameter name="schemaQualified" value="http://models.GeopoliticaRD.org.com"/>
      <parameter name="wsdlServicePort" value="Province"/>
      <parameter name="className" value="com.org.GeopoliticaRD.models.ProvinceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Province"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="helloWorld" qname="operNS:helloWorld" xmlns:operNS="http://models.GeopoliticaRD.org.com" returnQName="retNS:helloWorldReturn" xmlns:retNS="http://models.GeopoliticaRD.org.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="helloWorld"/>

  </service>
</deployment>
